let myName = 'Patrick'
let myAge = 29
// my age
let earlyYears = myAge - 2 * 10.5
// early years of age due to difference in dog years
let laterYears = (myAge - earlyYears) * 4
let myAgeInDogYears = earlyYears + laterYears
// console.log(`PATRICK `.toLowerCase() + myAgeInDogYears)
console.log(`My name is ${myName}. I am ${myAge} years old in human years which is ${myAgeInDogYears} years old in dog years.`)
